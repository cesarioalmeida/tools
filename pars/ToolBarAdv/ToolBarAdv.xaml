                                        </Border>
                                        <Border
                                            x:Name="PART_SystemMenu"
                                            Grid.ColumnSpan="3"
                                            HorizontalAlignment="Left"
                                            Background="Transparent"
                                            Visibility="Collapsed" />
                                    </Grid>
                                </Border>
                            </Grid>
                        </Border>

                        <Thumb
                            x:Name="PART_TopThumb"
                            Height="5"
                            Margin="0,1,0,0"
                            VerticalAlignment="Top"
                            Cursor="SizeNS"
                            Opacity="0" />
